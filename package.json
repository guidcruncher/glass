{
  "name": "my-glass-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "npm run format && npm run registry && vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "registry": "cd ./scripts && node ./generateGlassRegistry.ts",
    "preview": "vite preview",
    "build-only": "vite build",
    "format": "prettier . --write",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "topojson-client": "^3.1.0",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint-config-prettier": "^10.1.8",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.3",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  }
}
